def number_of_paths(dest_row, dest_col):
    if dest_row == 1 or dest_col == 1:
        return 1
    else:
        return number_of_paths(dest_row -1, dest_col) + number_of_paths(dest_row, dest_col -1)

print number_of_paths(2, 2)
        
